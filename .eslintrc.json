{
  "env": {
    "es6": false,
    "browser": true
  },
  "globals": {
    "sclivetickerAjax": "readonly",
    "wp": "readonly"
  },
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended",
    "plugin:@wordpress/eslint-plugin/es5"
  ],
  "overrides": [
    {
      "files": [
        "*"
      ],
      "rules": {
        "no-var": "off",
        "object-shorthand": "off"
      }
    }
  ]
}
